        "jquery:jQuery",
        "popper.js:popper"
      ]
    },
    "parsley": {
      "depends": [
        "jquery"
      ]
    },
    "jquery-form": {
      "depends": [
        "jquery"
      ]
    },
    "tempusdominius-bootstrap-4": {
