    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
    "popper": "./node_modules/popper.js/dist/umd/popper.js",
    "parsley": "./node_modules/parsleyjs/dist/parsley.js",
